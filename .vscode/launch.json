{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200/",
      "webRoot": "${workspaceFolder}",
      "skipFiles": [
        "node_modules/**/*.js",
        "**/styles.js",
        "**/runtime.js",
        "**/polifills.js",
        "**/main.js",
        "**/vendor.js",
        "lib/**/*.js",
        "async_hooks.js",
        "inspector_async_hook.js"
      ],
    },
  ]
}
